<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Enneto.Views.FlyoutPageShop"
             xmlns:pages="clr-namespace:Enneto.Views" 
                NavigationPage.HasNavigationBar="False"
            xmlns:pages1="clr-namespace:Enneto">

    <FlyoutPage.Flyout >
    <pages:FlyoutPageShopFlyout x:Name="FlyoutPage"/>
        
  </FlyoutPage.Flyout>
    
  <FlyoutPage.Detail >
    <NavigationPage >
      <x:Arguments>
                <pages1:MainPage />
      </x:Arguments>
    </NavigationPage >
  </FlyoutPage.Detail>
</FlyoutPage>